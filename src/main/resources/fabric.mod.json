{
  "schemaVersion": 1,
  "id": "creativetab",
  "version": "${version}",
  "name": "Gurkeor Creative Tab Addon",
  "description": "An addon to save/spawn custom items in creative.",
  "authors": [
    "JFronny",
    "Gurkenwerfer_"
  ],
  "contact": {
    "repo": "https://github.com/MeteorDevelopment/meteor-addon-template"
  },
  "icon": "assets/creativetab/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "de.creativetab.addon.CreativeTab"
    ],
    "main": ["de.creativetab.addon.CreativeTab::gameInit"]
  },
  "mixins": [
  ],
  "custom": {
    "meteor-client:color": "153,255,153"
  },
  "depends": {
    "java": ">=21",
    "minecraft": ["${mc_version}"],
    "meteor-client": "*"
  }
}
